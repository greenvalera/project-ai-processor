
{
    "extends": "airbnb-base",
    "parser": "@typescript-eslint/parser",
    "plugins": ["@typescript-eslint"],
    "rules": {
        "no-console": "off",
        "no-restricted-syntax": "off",
        "no-await-in-loop": "off",
        "arrow-parens": "off",
        "import/extensions": "off",
        "import/prefer-default-export": "off",
        "import/no-unresolved": "off"
    },
    "env": {
        "node": true,
        "es6": true
    }
}
